<div class="container">
        <%= link_to 'New brand', new_brand_path %>
        <br>
        <br>
        <table class="table table-bordered">
            <tr>
                <th>Brand Name</th>
                <th>Action</th>
            </tr>
            <% @brands.each do |brand| %>
                <tr>
                    <td><%= brand.name %></td>
                    <td><%= link_to 'Show', brand_path(brand) %></td>
                </tr>
            <% end %>
        </table>
</div>